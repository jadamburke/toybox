		print (`playblast -ae`+"\n");
		print ("ppPlayblastDoBlast "+$stereoCam+" \""+$pbPath+"\" \""+$fileName+"\" \"png\" "+`playbackOptions -q -min`+" "+`playbackOptions -q -max`+" "+$imgWidth+" "+$imgHeight+"\n");
		ppPlayblastDoBlast $stereoCam $pbPath $fileName "png" `playbackOptions -q -min` `playbackOptions -q -max` $imgWidth $imgHeight;



		// set render globals
		//setAttr "defaultRenderGlobals.animation" 1;
		//setAttr defaultRenderGlobals.outFormatControl 0;
		//setAttr "defaultRenderGlobals.extensionPadding" 4;


		// SGI image format
		//setAttr defaultRenderGlobals.imageFormat 5;

